{
  "name": "@debugmcp/adapter-rust",
  "version": "0.1.0",
  "description": "Rust Debug Adapter implementation for MCP Debugger using CodeLLDB",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "vendor"
  ],
  "scripts": {
    "build": "tsc",
    "build:adapter": "node scripts/vendor-codelldb.js",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist vendor/codelldb",
    "prepare": "npm run build:adapter",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@debugmcp/shared": "workspace:*",
    "@vscode/debugprotocol": "^1.65.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitest/ui": "^1.2.1",
    "extract-zip": "^2.0.1",
    "https-proxy-agent": "^7.0.2",
    "node-fetch": "^3.3.2",
    "progress": "^2.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "keywords": [
    "debug",
    "debugger",
    "rust",
    "codelldb",
    "lldb",
    "dap",
    "mcp"
  ],
  "author": "MCP Debugger Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/debugmcp/mcp-debugger.git",
    "directory": "packages/adapter-rust"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
